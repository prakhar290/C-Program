#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int num,sum=0;
    scanf("%d",&num);
    int *arr=(int*)malloc(num*sizeof(int));
    for(int i=0;i<num;i++)
    {
        scanf("%d",arr+i);
        sum=sum+arr[i];
    }   
    printf("%d",sum);
    return 0;
}
